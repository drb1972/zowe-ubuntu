{
   "$schema": "./zowe.schema.json",
   "profiles": {
       "base": {
           "type": "base",
           "properties": {
               "rejectUnauthorized": false
           },
           "secure": [
               "user",
               "password"
           ]
       },
       "marbles": {
           "type": "endevor-location",
           "properties": {
               "instance": "ENDEVOR",
               "environment": "DEV",
               "system": "MARBLES",
               "subsystem": "MARBLES"
           }
       },
       "slick": {
           "type": "endevor-location",
           "properties": {
               "instance": "ENDEVOR",
               "environment": "DEV",
               "system": "SLICK",
               "subsystem": "SLICK"
           }
       },
       "sr01": {
           "properties": {
               "host": "sr01brs.lvn.broadcom.net"
           },
           "profiles": {
               "zosmf": {
                   "type": "zosmf",
                   "properties": {
                       "port": 443
                   }
               },
               "tso": {
                   "type": "tso",
                   "properties": {
                       "codePage": "1047",
                       "logonProcedure": "IZUFPROC",
                       "account": "124400000"
                   }
               },
               "ssh": {
                   "type": "ssh",
                   "properties": {
                       "port": 22
                   }
               },
               "cics": {
                   "type": "cics",
                   "properties": {
                       "port": 6000,
                       "protocol": "https",
                       "regionName": "CICSTRN1"
                   }
               },
               "db2": {
                   "type": "db2",
                   "properties": {
                       "port": 6017,
                       "database": "D10CPTIB"
                   }
               },
               "endevor": {
                   "type": "endevor",
                   "properties": {
                       "port": 6032,
                       "protocol": "https"
                   }
               },
               "fmp": {
                   "type": "fmp",
                   "properties": {
                       "port": 6001,
                       "protocol": "https"
                   }
               },
               "jclcheck": {
                   "type": "jclcheck",
                   "properties": {
                       "port": "6028",
                       "jclcheckOptions": "NOAS NOAU NOHCD NOJCL NORES NOSIGN",
                       "protocol": "https"
                   }
               },
               "ops": {
                   "type": "ops",
                   "properties": {
                       "port": 6007,
                       "protocol": "https"
                   }
               },
               "zftp": {
                   "type": "zftp",
                   "properties": {
                       "port": 21,
                       "secureFtp": false
                   }
               }
           }
       }
   },
   "defaults": {
       "zosmf": "sr01.zosmf",
       "tso": "sr01.tso",
       "ssh": "sr01.ssh",
       "cics": "sr01.cics",
       "db2": "sr01.db2",
       "endevor": "sr01.endevor",
       "endevor-location": "marbles",
       "fmp": "sr01.fmp",
       "jclcheck": "sr01.jclcheck",
       "ops": "sr01.ops",
       "zftp": "sr01.zftp",
       "base": "base"
   },
   "autoStore": true
}